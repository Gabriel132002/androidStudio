<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/joinha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/joinha"
        />

    <TextView
        android:id="@+id/txtviewParabens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/joinha"
        android:layout_centerHorizontal="true"
        android:text="Parabéns, o extermindador \naprova sua persistência"
        android:textColor="@color/black"
        android:textSize="30dp" />

    <Button
        android:id="@+id/btnVoltarTela2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:backgroundTint="#0631AF"
        android:text="Voltar a tela inicial"
        />
    <TextView
        android:id="@+id/txtTela2Mostrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtviewParabens"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:text=""
        android:textSize="25dp"
        />
</RelativeLayout>